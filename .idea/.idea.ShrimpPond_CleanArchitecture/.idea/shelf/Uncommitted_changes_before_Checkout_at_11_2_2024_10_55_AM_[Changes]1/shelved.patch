+FROM mcr.microsoft.com/dotnet/aspnet:6.0 AS base
+WORKDIR /app
+EXPOSE 80
+EXPOSE 443
+
+FROM mcr.microsoft.com/dotnet/sdk:6.0 AS build
+ARG BUILD_CONFIGURATION=Release
+WORKDIR /src
+COPY ["ShrimpPondServer/ShrimpPondServer.csproj", "ShrimpPondServer/"]
+RUN dotnet restore "ShrimpPondServer/ShrimpPondServer.csproj"
+COPY . .
+WORKDIR "/src/ShrimpPondServer"
+RUN dotnet build "ShrimpPondServer.csproj" -c $BUILD_CONFIGURATION -o /app/build
+
+FROM build AS publish
+ARG BUILD_CONFIGURATION=Release
+RUN dotnet publish "ShrimpPondServer.csproj" -c $BUILD_CONFIGURATION -o /app/publish /p:UseAppHost=false
+
+FROM base AS final
+WORKDIR /app
+COPY --from=publish /app/publish .
+ENTRYPOINT ["dotnet", "ShrimpPondServer.dll"]
